        System.out.println("(4) KP03 - Fantastrike Mirage Impact");
        System.out.println("(5) MAX1 - Maximum Ultra Enhancement");
        System.out.println("(6) KP04 - Destined Power Destruction");
        System.out.println("(7) KP05 - Dynamic Eternal Live");
        System.out.println("(8) MAX2 - Maximum Ultra Evolution");
        System.out.println("(9) KP06 - Fierce Thunder Storm");
        System.out.println("(10) EXT1 - Extra Transcend Enhancement");
        System.out.println("(11) KP07 - Chaotic Omega Rising");
        System.out.println("(12) GRP1 - Gold Rush Pack");
        System.out.println("(13) KP08 - Genesis Master Road");
    }

    public static void openPack(int kp) {
        Random rand = new Random();
        int card;
        if (kp == 3 ||  kp == 5 || kp == 8 || kp == 10 || kp == 12) {

        }
        else {
            //Roll the commons (3)
            for (int i = 0; i < 3; i++) {
                card = rand.nextInt(KP_RARITY_DIST[kp][0]);
                System.out.println("Common: " + KP_LIST[kp][0][card]);
            }
            //Roll the rare (1)
            card = rand.nextInt(KP_RARITY_DIST[kp][1]);
            System.out.println("Rare: " + KP_LIST[kp][1][card]);
            //Roll the random card (3/4 SR, 1/6 UR, 1/12 RR)
            int roll = rand.nextInt(60);
            if (roll <= 44) {
                card = rand.nextInt(KP_RARITY_DIST[kp][2]);
                System.out.println("Super Rare: " + KP_LIST[kp][2][card]);
            }
            else if (roll >= 45 && roll <= 54) {
                card = rand.nextInt(KP_RARITY_DIST[kp][3]);
                System.out.println("Ultra Rare: " + KP_LIST[kp][3][card]);
            }
            else {
                card = rand.nextInt(KP_RARITY_DIST[kp][4]);
                System.out.println("Rush Rare: " + KP_LIST[kp][4][card]);
            }
        }
    }
}
