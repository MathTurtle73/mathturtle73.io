    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        menu();

        System.out.print("Which pack would you like to open? ");
        int pack = sc.nextInt();
        System.out.print("How many packs would you like to open? ");
        int num = sc.nextInt();
        for (int i = 0; i < num; i++) {
            System.out.println("--Pack " + (i + 1) + "--");
            openPack(pack - 1);
            System.out.println();
        }
    }

    public static void menu() {
        System.out.println("(1) KP01 - Hyperspeed Rush Road");
        System.out.println("(2) KP02 - Shocking Lightning Attack");
